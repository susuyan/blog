{
  "name": "blog",
  "version": "1.0.0",
  "description": "集博客之精华而生的目录，已有前端知识体系梳理、设计模式手册、Webpack4教程，欢迎食用",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "check": "node bin/prettier.js -t docs -c",
    "lint": "node bin/prettier.js -t docs -l",
    "check:notes": "node bin/prettier.js -t notes -c",
    "lint:notes": "node bin/prettier.js -t notes -l",
    "start": "npm run dev",
    "dev": "nodemon --watch docs bin/sidebar.js & ./node_modules/.bin/vuepress dev .",
    "build": "node bin/sidebar.js && ./node_modules/.bin/vuepress build ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/susuyan/susuyan.github.io.git"
  },
  "keywords": [],
  "author": "苏苏炎",
  "license": "MIT",
  "homepage": "https://github.com/susuyan/susuyan.github.io#readme",
  "dependencies": {
    "ejs": "^3.0.1",
    "ora": "^4.0.3",
    "tracer": "^1.0.1"
  },
  "devDependencies": {
    "vuepress": "^1.2.0"
  }
}
